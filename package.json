{
  "name": "etf-portfolio-backtesting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:smoke": "playwright test --grep='Smoke Tests'",
    "test:e2e:critical": "playwright test --grep='@critical'",
    "test:e2e:quick": "playwright test --grep='@quick'",
    "test:e2e:auth": "playwright test auth.spec.ts",
    "test:e2e:portfolio": "playwright test portfolio-creation.spec.ts",
    "test:e2e:backtest": "playwright test backtest*.spec.ts",
    "test:e2e:api": "playwright test backtest-api.spec.ts",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:serial": "playwright test --workers=1",
    "test:e2e:install": "playwright install",
    "test:e2e:setup": "./scripts/test-setup.sh",
    "test:e2e:verify": "./scripts/test-setup.sh --verify",
    "test:all": "npm run test && npm run test:e2e",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "config:validate": "node scripts/validate-config.js",
    "config:validate:dev": "node scripts/validate-config.js development",
    "config:validate:prod": "node scripts/validate-config.js production",
    "config:test": "node scripts/test-config.js",
    "config:setup": "npm run config:validate:dev && npm run config:test",
    "prebuild": "npm run config:validate:prod",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "check-app": "./scripts/check-and-restart.sh",
    "ensure-running": "lsof -i:3000 || PORT=3000 npm run dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/font": "^14.2.5",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.54.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.408.0",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "postcss": "^8.4.40",
    "prisma": "^5.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "recharts": "^2.12.7",
    "simple-statistics": "^7.8.3",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.2.7",
    "playwright": "^1.54.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "ts-jest": "^29.1.5",
    "wait-on": "^8.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
